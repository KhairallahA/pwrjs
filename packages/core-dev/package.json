{
    "name": "@pwrjs/core-dev",
    "version": "0.12.6",
    "description": "PWRJS is a javasrcipt library for interacting with the PWR network.",
    "keywords": [
        "web3",
        "cryptocurrency",
        "tokens",
        "pwr",
        "pwrchain",
        "blockchain",
        "secure"
    ],
    "author": "pwrlabs <devservices@pwrlabs.io>",
    "homepage": "https://www.pwrlabs.io",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pwrlabs/pwrjs.git"
    },
    "license": "ISC",
    "main": "./index.js",
    "files": [
        "src",
        "wallets",
        "index.js",
        "index.d.ts",
        "services.js",
        "services.d.ts",
        "types.js",
        "types.d.ts",
        "utils.js",
        "utils.d.ts"
    ],
    "exports": {
        ".": {
            "import": "./index.js",
            "require": "./index.js",
            "types": "./index.d.ts"
        },
        "./services": {
            "import": "./services.js",
            "require": "./services.js",
            "types": "./services.d.ts"
        },
        "./types": {
            "import": "./types.js",
            "require": "./types.js",
            "types": "./types.d.ts"
        },
        "./utils": {
            "import": "./utils.js",
            "require": "./utils.js",
            "types": "./utils.d.ts"
        },
        "./wallets/node": {
            "import": "./wallets/node.js",
            "require": "./wallets/node.js",
            "types": "./wallets/node.d.ts"
        },
        "./wallets/browser": {
            "import": "./wallets/browser.js",
            "require": "./wallets/browser.js",
            "types": "./wallets/browser.d.ts"
        },
        "./wallets/seedphrase": {
            "import": "./wallets/seedphrase.js",
            "require": "./wallets/seedphrase.js",
            "types": "./wallets/seedphrase.d.ts"
        }
    },
    "scripts": {
        "build": "npm run clean && npm run build:ts && npm run build:post",
        "clean": "node ../../scripts/clean.js packages/core",
        "build:post": "node ../../scripts/post-build.js packages/core",
        "dev": "npm build:ts --watch",
        "build:ts": "tsc -p tsconfig.json",
        "test": "vitest run"
    },
    "dependencies": {
        "@dashlane/pqc-sign-falcon-512-browser": "^1.0.0",
        "@dashlane/pqc-sign-falcon-512-node": "^1.0.0",
        "@noble/hashes": "^1.7.1",
        "axios": "^1.6.7",
        "bignumber.js": "^9.1.2",
        "bip39": "^3.1.0",
        "rust-falcon": "^0.2.6"
    },
    "devDependencies": {
        "@playwright/test": "^1.51.0",
        "@types/node": "^22.13.9",
        "ncp": "^2.0.0",
        "ts-jest": "^29.1.1",
        "typescript": "^5.2.2",
        "vitest": "^3.0.8"
    },
    "bugs": {
        "url": "https://github.com/pwrlabs/pwrjs/issues"
    },
    "publishConfig": {
        "access": "restricted"
    }
}
